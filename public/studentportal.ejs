<script>
	function marksheets_fetch(){
		alert("fetching");
		var xhttp = new XMLHttpRequest();
		xhttp.onreadystatechange = function(){
			if (this.readyState == 4 && this.status == 200) {
		   var res = this.responseText;
		   var obj = JSON.parse(res);
		   var str = "";
		  
		   var text= "<div>";
		   for(var i=0;i<obj.length;i++){
			str += `<div  style=" margin-top:63px; border:1.5px solid gainsboro; border-radius: 9px 9px 9px 9px; padding: 12px;">
							            <p style=" font-size: 26px; font-family: arial; float: left; color: black; " >`+obj[i].title+`</p><br>
										<p style=" font-size: 22px; font-family: arial; float: left; color: black; padding-top: 12px;" id="teacher_user">Name : `+obj[i].teacher_user+`</p>
										
										<img src="`+obj[i].pic_path+`" style="padding-top: 12px;" width="100%" alt="">
										<a href="`+obj[i].pdf_path+`" style="cursor: pointer;">
										<label style="float: left; height: 40px; line-height: 39px; border-radius: 9px 9px 9px 9px; margin-top: 12px; background-color: #abaeb1; border-color:#b8daff; opacity: 1; color: #2c2c31; width: 400px;">Marksheet Title</label><br><br>
									</a>
						</div>`
				 
		   }
		document.getElementById("marksheets").innerHTML = str;
	   
		}
	}
		xhttp.open("GET", "/students/marksheets", true);
	 xhttp.send();
	}
	function marksheet_show(){
		document.getElementById("marksheets").style.display= "block";
		document.getElementById("timetable").style.display= "none";

	}
	function timetable_show(){
		document.getElementById("marksheets").style.display= "none";
		document.getElementById("timetable").style.display= "block";


	}
	function marksheets_fetch2(email,password){
	 var xhttp = new XMLHttpRequest();
	 xhttp.onreadystatechange = function() {
	   if (this.readyState == 4 && this.status == 200) {
		   var res = this.responseText;
		   var obj = JSON.parse(res);
		   var text= "<div>";
		   for(var i=0;i<obj.length;i++){
			   text += `<h3>`+obj[i].title+`</h3><table  class="table table-bordered" >`;
				for (var j=0;j<obj[i].subjects.length;j++){
					text += `<tr><th>`+obj[i].subjects[j].name+`</th><td>`+obj[i].subjects[j].marks+`</td></tr>`
				}
				 
		   }
		   text += "</table></div>";
		document.getElementById("test").innerHTML = text;
	   }
	 };
	 xhttp.open("GET", "/student/marksheets?email="+email+"&password="+password, true);
	 xhttp.send();
	}
</script>
